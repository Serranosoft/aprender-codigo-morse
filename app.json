{
    "expo": {
        "name": "Codigo morse",
        "slug": "codigo-morse",
        "version": "1.1.2",
        "orientation": "portrait",
        "icon": "./assets/icono-morse-app.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": false,
        "splash": {
            "image": "./assets/splash-morse.png",
            "resizeMode": "contain",
            "backgroundColor": "#1D1F20"
        },
        "ios": {
            "supportsTablet": false,
            "bundleIdentifier": "com.paulaymanu113.morse",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false,
                "NSPhotoLibraryUsageDescription": "Permite a C칩digo Morse acceder a tus fotos"
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icono-morse-app.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "versionCode": 5,
            "package": "com.paulaymanu113.codigomorse"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-sqlite",
            [
                "react-native-google-mobile-ads",
                {
                    "androidAppId": "ca-app-pub-3738413299329691~2833775109",
                    "iosAppId": "ca-app-pub-3738413299329691~2833775109",
                    "delayAppMeasurementInit": true,
                    "userTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
                }
            ],
            [
                "expo-build-properties",
                {
                    "android": {
                        "extraProguardRules": "-keep class com.google.android.gms.internal.consent_sdk.** { *; }"
                    }
                }
            ],
            [
                "expo-camera",
                {
                    "cameraPermission": "Permite a C칩digo Morse acceder a tu c치mara",
                    "microphonePermission": "Permite a C칩digo Morse acceder a tu microfono"
                }
            ],
            "expo-localization",
            [
                "expo-font",
                {
                    "android": {
                        "fonts": [
                            {
                                "fontFamily": "Regular",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Regular.ttf",
                                        "weight": "400",
                                        "style": "normal"
                                    }
                                ]
                            },
                            {
                                "fontFamily": "Medium",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Medium.ttf",
                                        "weight": "500",
                                        "style": "normal"
                                    }
                                ]
                            },
                            {
                                "fontFamily": "Bold",
                                "fontDefinitions": [
                                    {
                                        "path": "./assets/fonts/AncizarSans-Bold.ttf",
                                        "weight": "600",
                                        "style": "normal"
                                    }
                                ]
                            }
                        ]
                    },
                    "ios": {
                        "fonts": [
                            "./assets/fonts/AncizarSans-Regular.ttf",
                            "./assets/fonts/AncizarSans-Medium.ttf",
                            "./assets/fonts/AncizarSans-Bold.ttf"
                        ]
                    }
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/notification.png"
                }
            ]
        ],
        "extra": {
            "router": {},
            "eas": {
                "projectId": "41df4eb2-829f-4c7d-8fef-f9a7e12dcf43"
            }
        }
    }
}